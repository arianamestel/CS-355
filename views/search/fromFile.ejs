<% include ../partials/header.ejs %>

	<div class='container'>
		<h3 class="d-flex justify-content-center text-secondary font-weight-bold">
			<span class="cabin">Search From File</span>
		</h3>
		<div class="input-group mb-3 mt-3  d-flex justify-content-center">
		 	<div class="custom-file col-md-6 col-sm-8">
		    	<input type="file" class="custom-file-input" id="inputFile" name="inputFile" >
		    	<label class="custom-file-label" for="inputFile" aria-describedby="submitInputFile" id="inputFileName" >Choose file</label>
		  	</div>
		  	<div class="input-group-append">
		    	<button class="input-group-text" id="submitInputFile">Upload</button>
		  	</div>
		</div>

		<h4 class="mt-3 mb-3" id="resultsTitle"></h4>
		<button id='checkAll' class='btn btn-outline-warning btn-sm mb-3' style="display: none">Check All</button>
		<button id='uncheckAll' class='btn btn-outline-info btn-sm mb-3' style="display: none">Uncheck All</button>
		<button id='saveModalButton' class='btn btn-danger btn-sm float-right' style="display: none" type="button" data-toggle="modal" data-target="#saveModal">Save & Download</button>

		<div id="results">

			
		</div>

		<% include downloadModal.ejs %>

	</div>

<% include ../partials/footer.ejs %>